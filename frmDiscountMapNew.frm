Private CN(10) As New clsData

Private Sub Form_Load()
    mdiOne.SetFormFont Me
    Me.Move 0, 0
    CN(0).dbOpen "select * from appview_DiscChart Order by 2,3", 1
    Set flxDiscChart.DataSource = CN(0).recs
    flxDiscChart.ColWidth(1) = 2500
    flxDiscChart.AutoSearch = flexSearchNone
End Sub

Private Sub txtName_LostFocus()
    If Trim(txtName) = "" Then txtName.Text = "GENERAL"
    CN(1).dbOpen "SELECT * FROM DiscTemplate WHERE DiscTpltName=" & QT(txtName.Text) & " ORDER BY PUBLISHERNAME, DISCGROUP", 1
    Set flxDiscTemplate.DataSource = CN(1).recs
End Sub

Private Sub flxDiscChart_EnterCell()
    If flxDiscChart.Col = 3 Then
        flxDiscChart.Editable = flexEDKbdMouse
        flxDiscChart.AutoSearch = flexSearchNone
    Else
        flxDiscChart.Editable = flexEDNone
        flxDiscChart.AutoSearch = flexSearchFromCursor
    End If
    CN(2).dbOpen "SELECT itemID, ISBN, itemNAME, PUBLISHERID AS PUBID, PUBLISHERNAME, PRICE, DiscGroup, PDISCOUNT, SDISCOUNT FROM itemS WHERE PUBLISHERID=" & QT(flxDiscChart.TextMatrix(flxDiscChart.Row, 0)) & " AND DiscGroup=" & QT(flxDiscChart.TextMatrix(flxDiscChart.Row, 2))
    Set flxitems.DataSource = CN(2).recs
    
End Sub

Private Sub flxDiscChart_AfterEdit(ByVal Row As Long, ByVal Col As Long)
        SQ = "appproc_SaveDiscountNew " & QT(txtName.Text)
        SQ = SQ & " ," & QT(flxDiscChart.TextMatrix(Row, 0))
        SQ = SQ & " ," & QT(flxDiscChart.TextMatrix(Row, 1))
        SQ = SQ & " ," & QT(flxDiscChart.TextMatrix(Row, 2))
        SQ = SQ & " ," & QT(flxDiscChart.TextMatrix(Row, 3))
        CN(3).dbOpen SQ, 1
        txtName_LostFocus
End Sub

